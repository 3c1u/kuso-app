{
    "name": "kuso-app",
    "bsc-flags": ["-bs-super-errors"],
    "sources": [
        {
            "dir": "resources/",
            "subdirs": true
        }
    ],
    "package-specs": [
        {
            "module": "es6",
            "in-source": true
        }
    ],
    "reason": { "react-jsx": 3 },
    "bs-dependencies": [
        "reason-react",
        "reductive",
        "bs-axios",
        "@3c1u/bs-inertia"
    ],
    "suffix": ".bs.js",
    "namespace": true,
    "ppx-flags": [],
    "refmt": 3,
    "gentypeconfig": {
        "language": "typescript",
        "module": "es6",
        "importPath": "relative",
        "shims": {
            "Js": "Js",
            "React": "ReactShim",
            "ReactEvent": "ReactEvent",
            "ReasonPervasives": "ReasonPervasives",
            "ReasonReact": "ReactShim"
        },
        "debug": {
            "all": false,
            "basic": false
        }
    }
}
